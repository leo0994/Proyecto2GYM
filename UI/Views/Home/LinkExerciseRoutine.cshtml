@{
    ViewData["Title"] = "Gym-Proyecto2";
}
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script src="~/js/linkExerciseRoutine.js" defer asp-append-version="true"></script>
<link rel="stylesheet" href="~/css/routineCreation.css" asp-append-version="true" />

    <h2>Assign Exercise to Routine</h2>


            <div class="table-container">

            <table id="ExerciseRoutine" class="display">
                <thead>
                    <tr>
                        <th>Base ID</th>
                        <th>ID Routine</th>
                        <th>ID Exercise ID</th>
                    </tr>
     
                </thead>
                <tbody>
                </tbody>
            </table>
        </div>


<div class="form-container">
<form id="linkRoutineForm">
    <div class="form-group">
        <label class="form-label" for="routineID">Routine ID:</label>
        <input class="form-input" type="text" class="form-control" id="routineID" name="routine" required>
    </div>
    <div class="form-group">   
        <label class="form-label" for="ExerciseID">Exercise ID to be assigned:</label>
        <input class="form-input" type="text" class="form-control" id="exerciseID" name="exercise" required>
    </div>
    <div class="form-group">   
    <button type="submit" class="btn btn-primary">Submit</button>
    </div>
</form>
</div>
<br>
